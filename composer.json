{
  "name": "miranj/craft-router",
  "description": "Use URL segments as filtering criteria on an entry query.",
  "type": "craft-plugin",
  "version": "1.4.1",
  "keywords": [
    "craft",
    "craftcms",
    "url",
    "router",
    "filters"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Miranj",
      "homepage": "https://miranj.in"
    }
  ],
  "support": {
    "docs": "https://github.com/miranj/craft-router/blob/v1/README.md",
    "issues": "https://github.com/miranj/craft-router/issues",
    "source": "https://github.com/miranj/craft-router"
  },
  "require": {
    "craftcms/cms": "^3.7.0 || ^4.0",
    "php": "^7.2.5 || ^8.0"
  },
  "require-dev": {
      "craftcms/rector": "dev-main"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "allow-plugins": {
          "yiisoft/yii2-composer": true,
          "craftcms/plugin-installer": true
      }
  },
  "autoload": {
    "psr-4": {
      "miranj\\router\\": "src/"
    }
  },
  "extra": {
    "name": "Router",
    "handle": "router",
    "changelogUrl": "https://raw.githubusercontent.com/miranj/craft-router/v1/CHANGELOG.md",
    "class": "miranj\\router\\Plugin"
  }
}
